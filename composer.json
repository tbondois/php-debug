{
    "name": "tbondois/php-debug",
    "description": "PHP debug libraries (dumpers...) for common needs. should not be installed in production or use IP restriction feature.",
    "require": {
        "php": ">=7.0",
        "kint-php/kint": "^3.2",
        "kint-php/kint-js": "^2.0",
        "symfony/var-dumper": ">=2.8",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-ctype": "*"
    },
    "minimum-stability": "stable",
    "suggest": {
        "tbondois/php-toolbox": "PHP libraries for common needs",
        "symfony/dotenv": "An easy way to manage IP restriction for debug features, in sensitive environments",
        "symfony/debug": "Allow you to easily catch PHP errors, uncaught exceptions, class loader errors and display them nicely",
        "maximebf/debugbar": "A generic PHP Debug toolbar monitoring errors and exception, sql queries, ajax etc"
    },
    "autoload": {
        "psr-4": {
            "TB\\Debug\\": "src/"
        },
        "files": [
            "init/aliases.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TB\\Debug\\Test\\": "tests/"
        }
    },
    "extra": {
        "kint": {
            "disable-facade": true,
            "disable-helpers": true
        }
    },
    "license": "MIT",
    "homepage": "https://packagist.org/packages/tbondois/php-debug",
    "authors": [
        {
            "name": "Thomas Bondois",
            "homepage": "http://thomas.bondois.info"
        }
    ],
    "keywords": [
        "debug",
        "varprint",
        "printer",
        "print",
        "dump",
        "dumper",
        "php"
    ]
}
